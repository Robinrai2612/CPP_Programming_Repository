class Matrix
{
public:
    vector<vector<int>> a;

    Matrix operator+(Matrix& A) {
        Matrix result;
        for (int i{ 0 }; i < A.a.size(); ++i) {
            vector<int> temp;
            for (int j{ 0 }; j < A.a[0].size(); ++j) {
                temp.push_back(this->a[i][j] + A.a[i][j]);
            }
            result.a.push_back(temp);
        }
        return result;
    }
};
